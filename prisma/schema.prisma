generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Credentials {
  key   String @id
  value String
}

model SmartsheetRow {
  sheetId      String
  siblingId    String?
  version      Int
  expanded     Boolean
  accessLevel  String
  createdAt    String
  modifiedAt   String
  smartsheetId String           @id
  basecampId   String?
  rowNumber    Int
  cells        SmartsheetCell[]
}

model SmartsheetCell {
  id              String         @id @default(cuid())
  columnId        String
  value           String?
  displayValue    String?
  smartsheetRowId String?
  SmartsheetRow   SmartsheetRow? @relation(fields: [smartsheetRowId], references: [smartsheetId])
}
